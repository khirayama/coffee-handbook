{
  "name": "coffee",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf src/public",
    "start": "npm run clean && npm run generate && npm run bundle:scripts && npm run build:styles && NODE_PATH=src node src/index.js",
    "dev": "npm run clean && npm run generate && concurrently 'npm run bundle:scripts:watch' 'npm run build:styles:watch' 'NODE_PATH=src nodemon src/index.js'",
    "generate": "node ./bin/generator.js data/posts -o src/data/posts/index.json -m data/meta",
    "bundle:scripts": "NODE_ENV=production webpack --mode production",
    "bundle:scripts:watch": "webpack -w --mode development",
    "bundle:scripts:analyze": "NODE_ENV=production ANALYSIS=true webpack --mode production",
    "build:styles": "postcss -c .postcssrc.js 'src/presentations/**/index.css' --base src/presentations --dir './src/public'",
    "build:styles:watch": "postcss -c .postcssrc.js -w 'src/presentations/**/index.css' --base src/presentations --dir './src/public'",
    "build:images": "node bin/minimize-images.js",
    "compress:images": "node bin/minimize-images.js",
    "lint": "eslint --ignore-path .gitignore src bin webpack.config.js",
    "lint:fix": "eslint --fix --ignore-path .gitignore src bin webpack.config.js",
    "format": "prettier --ignore-path .gitignore --config .prettierrc --write 'src/**/!(*.json|*.jpg|*.pug|*.html|*.png|*.webp|*.svg)' bin webpack.config.js .eslintrc.js .postcssrc.js",
    "precommit": "npm run lint:fix && npm run format",
    "check": "npx npm-check-updates"
  },
  "keywords": [],
  "author": "khirayama",
  "license": "",
  "dependencies": {
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "express": "4.16.3",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.5.1",
    "cssnano": "3.10.0",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-xo-space": "0.18.0",
    "eslint-plugin-prettier": "2.6.0",
    "glob": "7.1.2",
    "imagemin": "5.3.1",
    "imagemin-jpegtran": "5.0.2",
    "imagemin-pngquant": "5.1.0",
    "imagemin-webp": "4.1.0",
    "minimist": "1.2.0",
    "nodemon": "1.17.5",
    "postcss-cli": "5.0.0",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "postcss-simple-vars": "4.1.0",
    "prettier": "1.13.5",
    "rss": "1.2.2",
    "ts-loader": "4.4.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "2.9.2",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.0.4"
  }
}
